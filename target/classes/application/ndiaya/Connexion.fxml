            <Font size="16.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordTxt" layoutX="19.0" layoutY="130.0" prefHeight="39.0" prefWidth="251.0" promptText="Mot de Passe">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <Button fx:id="connexionBtn" layoutX="93.0" layoutY="195.0" mnemonicParsing="false" onAction="#onConnect" prefHeight="39.0" prefWidth="104.0" text="Connexion" />
      <Label layoutX="87.0" layoutY="281.0" text="Pas de compte ?">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="inscriptionBtn" layoutX="93.0" layoutY="313.0" mnemonicParsing="false" onAction="#openInscriptionFn" prefHeight="39.0" prefWidth="104.0" text="S'inscrire" />
