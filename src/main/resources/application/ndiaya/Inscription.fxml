<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="564.0" prefWidth="321.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ndiaya.RegisterController">
   <children>
      <Label layoutX="120.0" layoutY="22.0" text="Inscription">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="nomTxt" layoutX="17.0" layoutY="70.0" prefHeight="38.0" prefWidth="290.0" promptText="Nom">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="prenomTxt" layoutX="17.0" layoutY="123.0" prefHeight="38.0" prefWidth="290.0" promptText="Prenom">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="telephoneTxt" layoutX="17.0" layoutY="178.0" prefHeight="38.0" prefWidth="290.0" promptText="Telephone">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="emailTxt" layoutX="17.0" layoutY="230.0" prefHeight="38.0" prefWidth="290.0" promptText="Email">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="loginTxt" layoutX="15.0" layoutY="284.0" prefHeight="38.0" prefWidth="290.0" promptText="Login">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordTxt" layoutX="17.0" layoutY="336.0" prefHeight="38.0" prefWidth="290.0" promptText="Mot de Passe">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button fx:id="inscriptionBtn" layoutX="105.0" layoutY="405.0" mnemonicParsing="false" onAction="#onRegister" prefHeight="38.0" prefWidth="110.0" text="S'inscrire" />
      <Label layoutX="85.0" layoutY="468.0" text="Vous avez un compte ?">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="connexionBtn" layoutX="107.0" layoutY="502.0" mnemonicParsing="false" onAction="#openConnexionFn" prefHeight="38.0" prefWidth="110.0" text="Se connecter" />
